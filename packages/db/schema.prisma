generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Url {
  id        Int      @id @default(autoincrement())
  slug      String   @unique
  original  String
  password  String?
  clicks    Int      @default(0)
  expiresAt DateTime
  createdAt DateTime @default(now())

  @@index([expiresAt])
} 